{
  "extends": [
    "plugin:@typescript-eslint/recommended",
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "node": true
  },
  "rules": {
    "require-await": "error",
    "@typescript-eslint/ban-ts-comment": "off",
    "import/no-anonymous-default-export": "off",
    "no-unused-vars": "warn"
  }
}
